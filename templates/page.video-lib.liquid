var setVideo = function (x) {
      $("#"+x).html('<iframe width="560" height="315" style="width:100%" src="' + videos[x].video + '" frameborder="0" allowfullscreen></iframe>');
}
  function toggle(x) {

    if ($('#'+x).hasClass('dn')) {

        $('#'+x).removeClass('dn');
        $('#descr'+x).removeClass('dn');
      	$('#'+x+'Link').addClass('bg-red');
      	$('#'+x+'Link').addClass('white');
       	$('#'+x+'Link').removeClass('exerciseList');

    } else {
        $('#'+x).addClass('dn');
        $('#descr'+x).addClass('dn');
      	$('#'+x+'Link').removeClass('bg-red');
      	$('#'+x+'Link').removeClass('white');
      	$('#'+x+'Link').addClass('exerciseList');
                  	setVideo(x)
    }
}
  function clearInput(){
    $('#exerciseSearch').val('');
    selectExercises();
  }
var stateOfProgramFilter='all';
function selectExercises() {
    var input, filter, ul, li, span, i, shownList;
    input = document.getElementById("exerciseSearch");
    filter = input.value.toUpperCase();
  	filter=filter.trim();
  	if (stateOfProgramFilter === 'all') {
      shownList='exerciseList'
    } else if (stateOfProgramFilter === 'leg') {
      shownList='legList'
    } else if (stateOfProgramFilter === 'basic') {
      shownList='basicList'
    }
  	ul = document.getElementById('exerciseList');
  	li = $('ul#exerciseList > li')
    var visibleN=0;
    for (i = 0; i < li.length; i++) {
        span = li[i].getElementsByTagName("span")[0];
      if (span.classList.contains(shownList) === true) {
        visibleN++;
        var y=span.id.slice(0,-4);
        

        if (span.innerHTML.toUpperCase().indexOf(filter) > -1) {
            li[i].style.display = "";
          
          if (filter=='') {
            $('#'+y).addClass('dn');
            $('#descr'+y).addClass('dn');
      		$('#'+span.id).removeClass('bg-red');
      		$('#'+span.id).removeClass('white');
      		$('#'+span.id).addClass('exerciseList');
      		setVideo(y)
          }

            
        } else {
            li[i].style.display = "none";
            $('#'+y).addClass('dn');
            $('#descr'+y).addClass('dn');
      		$('#'+span.id).removeClass('bg-red');
      		$('#'+span.id).removeClass('white');
      		$('#'+span.id).addClass('exerciseList');
      		setVideo(y)

        }
      } else {
            li[i].style.display = "none";
            $('#'+y).addClass('dn');
            $('#descr'+y).addClass('dn');
      		$('#'+span.id).removeClass('bg-red');
      		$('#'+span.id).removeClass('white');
      		$('#'+span.id).addClass('exerciseList');
      }
    }
}
  function changeLang(lang) {
    var otherlang;
    if (lang === "russian") {
      otherlang = "english"
    } else if (lang === "english") {
	  otherlang = "russian"
    }
    $('#'+lang).addClass('red')
    $('#'+otherlang).removeClass('red');
      
    if ($("div."+lang+":visible").length === 0) {
		$("."+otherlang).hide();
		$("."+lang).show();
    } else {
		$("."+lang).hide();
		$("."+otherlang).show();
    }
  }
  function showVideosFromProgram(program) {
    if (program === "all") {
      stateOfProgramFilter='all';
      $('#all').addClass('red');
      $('#leg').removeClass('red');
      $('#basic').removeClass('red');


    } else if (program === "leg") {
      stateOfProgramFilter='leg';
      $('#leg').addClass('red');
      $('#all').removeClass('red');
      $('#basic').removeClass('red');

    } else if (program === "basic") {
      stateOfProgramFilter='basic';
      $('#basic').addClass('red');
      $('#leg').removeClass('red');
      $('#all').removeClass('red');

    }
    selectExercises()
  }
</script>
      
      
    	
      
