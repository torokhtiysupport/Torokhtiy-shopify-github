                  <script>
                    document.addEventListener('DOMContentLoaded', () => {
              
                      platesCalculator();
                      function platesCalculator() {
                    
                        const form              = document.querySelector('[data-gym-form]')
                        const inputs            = form.querySelectorAll('[data-income]');
                        const unitsSelects      = form.querySelectorAll('[data-change-unit]');
                        const unitsForms        = form.querySelectorAll('[data-units-form');
                        const resultsSection    = document.querySelector('[data-results]');
                        const resultsField      = document.querySelector('[data-results-field]');
                    
                        let unit                = 'kg';
                    
                        unitsSelects.forEach(select => {
                          select.addEventListener('input', (evt) => {
                            changeUnit(evt.target.value);
                          });
                        })
                    
                        // inputs.forEach(input => {
                        //   input.addEventListener('input', calculate);
                        // });
                        form.addEventListener('submit', (evt) => {
                          evt.preventDefault();
                          calculate();
                        });
                        // calculate();
                    
                        function collectPlates() {
                          const platesInputs = document.querySelectorAll('[data-units-form].active [data-plate]');
                          const plates = [];
                    
                          platesInputs.forEach(input => {
                            plates.push({[input.dataset.plate]: input.valueAsNumber});
                          });
                    
                          return plates;
                        }
                    
                        function changeUnit(targetUnit) {
                          unit = targetUnit;
                    
                          unitsSelects.forEach(select => {
                            select.value = unit;
                          });
                    
                          unitsForms.forEach(form => {
                            if (form.dataset.unitsForm === targetUnit) {
                              form.classList.add('active')
                            } else {
                              form.classList.remove('active')
                            }
                          });
                        }
                    
                        function calculateVariations(sideWeight, availablePlates) {
                          const variations = [];
                    
                          findWay(sideWeight, []);
                          function findWay(remainder, accumulator) {
                            for (let i = 0; i < availablePlates.length; i++) {
                              if (remainder === 0) break;
                              const plate = availablePlates[i];
                              const plateKey = Object.keys(plate)[0];
                              const plateWeight = +plateKey;
                              let plateCount = plate[plateKey];
                    
                              while (plateCount >= 2) {
                                plateCount -= 2;
                                const result = remainder - plateWeight;
                                if (result < 0) {
                                  break;
                                }
                    
                                remainder = result;
                                accumulator.push(plateWeight);
                              }
                            }
                            variations.push({remainder, usedPlates: accumulator});
                          }
                    
                          return variations;
                        }
                    
                        function calculate() {
                          const totalLift = document.querySelector('[data-income="TL"]').valueAsNumber;
                          const barWeight = document.querySelector('[data-income="BW"]').valueAsNumber;
                    
                          if (totalLift < barWeight) {
                            showResult('The total lift cannot be less than the weight of the bar!', {
                              targetLift: totalLift,
                              totalLift: barWeight,
                              barWeight,
                              usedPlates: []
                            });
                            return;
                          }
                    
                          const availablePlates = collectPlates();
                          const sideWeight = (totalLift - barWeight) / 2;
                    
                          const variations = calculateVariations(sideWeight, availablePlates);
                          const bestWay = findBestWay(variations);
                    
                          showResult(null, {
                            targetLift: totalLift,
                            totalLift: barWeight + bestWay.remainder,
                            barWeight,
                            usedPlates: bestWay.usedPlates
                          });
                        }
                    
                        function findBestWay(variations) {
                          const remainder = variations[0].remainder;
                          const usedPlates = variations[0].usedPlates;
                          let totalLift = 0;
                    
                          const filteredUsedPlates = usedPlates.reduce((prev, curr) => {
                            totalLift += curr;
                    
                            const previousPlate = prev[prev.length-1];
                            if (!prev.length) {
                              prev.push({[curr]: 1});
                              return prev;
                            }
                    
                            if (Object.keys(previousPlate)[0] == curr) {
                              previousPlate[curr]++;
                            } else {
                              prev.push({[curr]: 1});
                            }
                            return prev;
                          }, []);
                    
                          totalLift *= 2;
                    
                          return {
                            remainder: totalLift,
                            usedPlates: filteredUsedPlates
                          };
                        }
                    
                        function showResult(err, {targetLift, totalLift, barWeight, usedPlates}) {
                          resultsSection.classList.add('active');
                          window.scrollTo({
                            top: resultsSection.getBoundingClientRect().top - 100,
                            behavior: 'smooth'
                          });
                    
                          if (err) {
                            resultsField.innerHTML = `<p>${err}<p>`;
                            return;
                          }
                    
                          const platesCount = usedPlates.reduce((prev, curr) => {
                            return prev + Object.values(curr)[0];
                          }, 0);
                    
                          if (totalLift < targetLift) {
                            resultsField.innerHTML = `<p>We couldn't make your lift exactly but you can make <b>${totalLift} ${unit}</b> from <b>${platesCount} plates</b> each side plus the bar weight of <b>${barWeight} ${unit}</b>.</p>`;
                            resultsField.innerHTML += `<p>This is <b>${targetLift - totalLift} ${unit}</b> under your target of <b>${targetLift} ${unit}</b>.</p>`;
                          } else {
                            resultsField.innerHTML = `<p>You can make <b>${totalLift} ${unit}</b> from <b>${platesCount} plates</b> each side plus the bar weight of <b>${barWeight} ${unit}</b>.</p>`;
                          }
                          
                          resultsField.innerHTML += '<h3>Each side</h3>';
                    
                          const list = document.createElement('ul');
                          usedPlates.forEach(plate => {
                            const li = document.createElement('li');
                            li.innerText = `${Object.values(plate)[0]} x ${Object.keys(plate)[0]} ${unit}`;
                            list.append(li);
                          });
                          resultsField.append(list);
                          
                          resultsField.innerHTML += `<br><p>Bar: ${barWeight} ${unit}</p>`;
                        }
                    
                      }
                    
                    });
                  </script>
              
