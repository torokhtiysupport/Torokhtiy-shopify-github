                {{ paginate | default_pagination | replace: '&laquo; Previous', '&larr;' | replace: 'Next &raquo;', '&rarr;' }}
              </div>
            {% endif %}
          {% endif %}
        </div>
      {% endpaginate %}
    </div>
  {% endif %}  <div class="content-block">
    <div class="comment-form form-vertical">
      {% form 'new_comment', article %}        <h2 class="h3">{{ 'blogs.comments.title' | t }}</h2>        {{ form.errors | default_errors }}        <div class="grid grid--half-gutters">
          <div class="grid__item medium-up--one-half">
            <label for="CommentAuthor" class="label--hidden">{{ 'blogs.comments.name' | t }}</label>
            <input type="text" name="comment[author]" id="CommentAuthor" class="{% if form.errors contains 'author' %} input--error{% endif %}" placeholder="{{ 'blogs.comments.name' | t }}" value="{{ form.author }}">
          </div>          <div class="grid__item medium-up--one-half">
            <label for="CommentEmail" class="label--hidden">{{ 'blogs.comments.email' | t }}</label>
            <input type="email" name="comment[email]" id="CommentEmail" class="{% if form.errors contains 'email' %} input--error{% endif %}" placeholder="{{ 'blogs.comments.email' | t }}" value="{{ form.email }}" autocorrect="off" autocapitalize="off">
          </div>
        </div>        <label for="CommentBody" class="label--hidden">{{ 'blogs.comments.message' | t }}</label>
        <textarea name="comment[body]" id="CommentBody" class="{% if form.errors contains 'body' %} input--error{% endif %}" placeholder="{{ 'blogs.comments.message' | t }}">{{ form.body }}</textarea>        <input type="submit" onClick='$.ajax({
                	url: "https://hooks.zapier.com/hooks/catch/2032528/s6d11m/", // url where to submit the request
                	type : "POST", // type of action POST || GET
                	dataType : "json", // data type
                	data : $("#comment_form").serialize()+"&article="+"{{article.title}}",  // post data || get data
                	success : function(result) {
                    // you can see the result from the console
                    // tab of the developer tools
                	},
                	error: function(xhr, resp, text) {
                    	console.log(xhr, resp, text);
                	}
            	})' class="btn" value="{{ 'blogs.comments.post' | t }}">        {% if blog.moderated? %}
          <p class="text-center"><small>{{ 'blogs.comments.moderated' | t }}</small></p>
        {% endif %}
      {% endform %}
    </div>
  </div>
{% endif %}      {% section 'featured-products-subsection' %}      <div class="full-width full-width--return-link">
        <a href="{{ blog.url }}" class="h1 return-link">
          {% include 'icon-arrow-thin-left' %}
          {{ 'blogs.article.back_to_blog' | t: title: blog.title }}
        </a>
      </div>
    </div>    {% if blog.all_tags.size > 0 %}
      <div class="grid__item medium-up--one-quarter large-up--one-fifth small--hide">
        <div class="content-block content-block--small">
          <h3 class="h6 content-block__title">{{ 'blogs.article.tags' | t }}</h3>
          <ul class="list--no-bullets list--bold">
            {% for tag in blog.all_tags %}
              {% if current_tags contains tag %}
              <li>{{ tag }}</li>
              {% else %}
              <li>{{ tag | link_to_tag: tag }}</li>
              {% endif %}
            {% endfor %}
          </ul>
        </div>
      </div>
    {% endif %}  </article>
</div>
