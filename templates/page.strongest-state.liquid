<script>
    async function fetchData(url, container) {
        //const url = url; // замініть посилання на своє
		const response = await fetch(url);
		const data = await response.json();
        const columnNames = {
		  "State": "State",
		  "Wilks average": "Wilks average",
		  "MEN Best Squat Kg": "MEN Best Squat (Kg)",
		  "MEN Best Bench Kg": "MEN Best Bench (Kg)",
		  "MEN Best Deadlift Kg": "MEN Best Deadlift (Kg)",
		  "WOMEN Best Squat Kg": "WOMEN Best Squat (Kg)",
		  "WOMEN Best Bench Kg": "WOMEN Best Bench (Kg)",
		  "WOMEN Best Deadlift Kg": "WOMEN Best Deadlift (Kg)",
          "Name":"Name",
          "Wilks":"Wilks",
          "Best Squat (Kg)":"Best Squat (Kg)",
          "Best Bench (Kg)":"Best Bench (Kg)",
          "Best Deadlift (Kg)":"Best Deadlift (Kg)",
          "Total (Kg)":"Total (Kg)",
          "Bodyweight (Kg)":"Bodyweight (Kg)",
          "Best3Bench (Kg)":"Best3Bench (Kg)",
		};
        if (data.length > 0) {
			// Отримуємо назви колонок з ключів першого об'єкту
			const headers = Object.keys(data[0]);

			// Створюємо масив об'єктів даних для footable з використанням назв колонок
            const footableData = data.map(row => {
              const rowData = {};
