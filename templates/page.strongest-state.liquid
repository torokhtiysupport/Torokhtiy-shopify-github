<script>  
  
  async function fetchAndRenderDataMap(url, weightType, containerId) {
  
  //const response = await fetch('{{ 'strongest-state.json' | asset_url | prepend: '//torokhtiy.com' | url }}');  
  const response = await fetch(url); 
  const data = await response.json();
  const weightMultiplier = weightType === "kg" ? 1 : 2.20462;
  const result = {};
  
  // ...
  
  for (let i = 0; i < data.length; i++) {
    const row = data[i];
    
    // Отримуємо необхідні дані з об'єкта
    const stateCode = row.State;
    
    const wilks_average = row["Wilks average"];
    
    const squat_m = (row["MEN Best Squat Kg"] * weightMultiplier).toFixed(2);
    const bench_m = (row["MEN Best Bench Kg"] * weightMultiplier).toFixed(2);
    const deadlift_m = (row["MEN Best Deadlift Kg"] * weightMultiplier).toFixed(2);
    
    const squat_w = (row["WOMEN Best Squat Kg"] * weightMultiplier).toFixed(2);
    const bench_w = (row["WOMEN Best Bench Kg"] * weightMultiplier).toFixed(2);
    const deadlift_w = (row["WOMEN Best Deadlift Kg"] * weightMultiplier).toFixed(2);

    // Додаємо новий запис до об'єкту
    result[stateCode] = {
      wilks_average,
      squat_m,
      bench_m,
      deadlift_m,
      squat_w,
      bench_w,
      deadlift_w,
    };
  }

  const election = new Datamap({
    scope: 'usa',
    element: document.getElementById(containerId),
    geographyConfig: {
      highlightBorderColor: '#fff',
      highlightFillColor: '#1C6EA4',
      popupTemplate: function(geography, data) {
        var i = '<div class="stats-layer '+weightType+'">';
        return i += '<h4>'+ geography.properties.name +'</h4><div class="stats"> <strong>Strength score (by Wilks average):</strong> <br> '+data.wilks_average+'</div><div class="stats"> <strong>MEN Best Results:</strong> <br> Squat: '+data.squat_m+' '+weightType+' <br> Bench Press: '+data.bench_m+' '+weightType+' <br> Deadlift: '+data.deadlift_m+' '+weightType+' </div><div class="stats"> <strong>WOMEN Best Results:</strong> <br> Squat: '+data.squat_w+' '+weightType+' <br> Bench Press: '+data.bench_w+' '+weightType+' <br> Deadlift: '+data.deadlift_w+' '+weightType+' </div></div></div>';
      },
      highlightBorderOpacity: 1,
      highlightBorderWidth: 1,
      popupOnHover: !0,
      highlightOnHover: !0
    },
    fills: {
      defaultFill: '#EEE7DB'
    },
    data: result
  });

  election.labels();

  return election;
}

  
let selectedWeightType = 'kg';

function handleWeightTypeChange() {
