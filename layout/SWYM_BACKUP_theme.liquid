  <div id="id01" class="w3-modal">
    <div class="w3-modal-content mw6 w3-animate-opacity w3-card-4 br4">
      <div class="cf w-100 h-100 br3">
        <div class="fl w-100 emailSensei cover br3 pt5-ns pt5  ph3" >
		  <p class="avenir fw5 f2-ns f4 mv0 white tc" style="line-height: 1.2;padding-top:7rem;">Get Free PDF with TOP 5 Weightlifting Mistakes and Ways to Fix Them</p>
          <form id="subscription" class="cf w-100" action="javascript:void(0);">
            <div id="subscriptionInput" class="cf mw7-ns center mw6 pa2 w-100">
              <p id="errorInSubscrEmail" class="hide white">Please enter a valid e-mail</p>
              <p id="confirmEmailSent" class="hide white">You are subscribed! Stay tuned</p>

            <i class="f1 w-20 tc  fl fa fa-envelope-o white" aria-hidden="true" style="font-size:2rem"></i>
            <input type="email" id="subscriptionEmail" name="subscriptionEmail" class="fr br3  center tl w-80" placeholder="Enter your best email"/>
            </div>
            <div class="cf w-100">
              <div class="fr-ns cf w-50-ns pa2 mw6 center w-100">
             <input id="submitSubscriptionEmail" class="w-100 pa2 bg-red ba b--red white fw6 subscriptionBtnYes" type="button"  name="submit" value="Get My PDF">
              </div>
              <div class="fr-ns cf w-50-ns pa2 mw6 center w-100">
                <button class="w-100 pa2 white subscriptionBtnNo" onclick="closeModal()">No, thanks</button>
              </div>
            </div>

          </form>
		</div>

      </div>
    </div>
  </div>
</div>
  <script>
  function closeModal() {
  	$('#id01').css('display','none')
	// Store
	localStorage.setItem("emailModal", "closed");
  }
    
  $(document).ready(function(){
    if (localStorage.getItem("emailModal")!="closed") {
      setTimeout(function(){
		document.getElementById('id01').style.display='block';
   	  },13000); 
    }
        // click on button submit
    $("#submitSubscriptionEmail").on('click', function(){
      var email=$('#subscriptionEmail').val();
          
      if (email.indexOf("@")==-1) {
        $("#confirmEmailSent").addClass("hide");  
        $("#errorInSubscrEmail").removeClass("hide")
      } else {
		$("#errorInSubscrEmail").addClass("hide")

        $.ajax({
          url: 'https://hooks.zapier.com/hooks/catch/2032528/19ubgl/', // url where to submit the request
          type : "POST", // type of action POST || GET
          dataType : 'json', // data type
          data : $("#subscription").serialize(),  // post data || get data
          success : function(result) {
            $("#confirmEmailSent").removeClass("hide"); 
            setTimeout(function(){
              closeModal()
 			},1000); 
          },
          error: function(xhr, resp, text) {
            console.log(xhr, resp, text);
          }
        })               
                  
       } 
     });
        
    });
  </script>
</body>
</html>
